{
  "code-runner.runInTerminal": true,
  "code-runner.showExecutionMessage": false,
  "code-runner.clearPreviousOutput": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "typescript": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' node_modules/.bin/\"$(node ./scripts/custom-code-runner.js $fileName)\" $fullFileName",
  },
  "code-runner.executorMapByFileExtension": {
    ".tsx": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' node_modules/.bin/\"$(node ./scripts/custom-code-runner.js $fileName)\" $fullFileName",
  },
  "code-runner.ignoreSelection": true,
  "code-runner.preserveFocus": true,
  "code-runner.saveFileBeforeRun": true,
  "prettier.requireConfig": true,
  "prettier.tslintIntegration": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    ".next/**": true,
    ".tmp/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    ".next": true
  },
  "cSpell.words": [
    "electron",
    "estudo",
    "lamprecht",
    "nextsjs",
    "ts"
  ]
}
